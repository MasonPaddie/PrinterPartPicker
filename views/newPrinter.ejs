<%- include('./partials/header.ejs') %>
    <form action="/printers" method="POST">
        <h1>New Printer Entry</h1>

        <!-- dropdown for type -->
        <label id="basePrinterDropdown" for="basePrinter">Base Printer:</label>
        <select name="basePrinter" >
           <% for (let i = 0; i < printers.length; i++) {%>
            <option value="<%= printers[i].name %>"> <%= printers[i].name %> </option>
            <% } %>
        </select> <br><br>

        <input type="submit" value="Make New Printer With Selected Printer As A Base">
    </form>
<%- include('./partials/footer.ejs') %>